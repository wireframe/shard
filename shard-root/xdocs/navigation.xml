<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="$pom.name">

  <title>$pom.name</title>

  <body>
    <links>
      <item name="Manual" href="${pom.artifactId}.pdf" />
    </links>
    
  <!-- see multiproject-plugin/plugin-resources/navigation-aggrigate.xml -->
  <!-- createOverviewPage  -->
    <menu name="Projects">
    #if ($overviewPageCreate == "true" )
      <item name="${overviewPageLinkTitle}" href="${overviewPageLink}.html"/>
    #end
    #foreach ($reactorProject in $multiprojects)
      <item name="$reactorProject.name" href="/${aggregateDir}${reactorProject.artifactId}/index.html"/>
    #end
    </menu>
    
    <menu name="Documentation">
      <item name="License" href="license.html" />
      <item name="Wiki" href="http://www.codecrate.com/display/SHA/Home" />
      <item name="Forums" href="http://forum.codecrate.com" />
      <item name="Issues/Roadmap" href="http://jira.codecrate.com/secure/BrowseProject.jspa?id=10002" />
    </menu>
    
    <search/> 
  </body>
</project>